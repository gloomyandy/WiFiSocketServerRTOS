CONFIG_LWIP_SO_REUSE=y

CONFIG_PARTITION_TABLE_CUSTOM=y

CONFIG_ENABLE_MDNS=y

CONFIG_NEWLIB_NANO_FORMAT=n
CONFIG_LOG_DEFAULT_LEVEL_ERROR=y
CONFIG_LOG_BOOTLOADER_LEVEL_ERROR=y
CONFIG_FREERTOS_HZ=1000


# ESP8266-Specific config
CONFIG_ESP_PHY_INIT_DATA_VDD33_CONST=255
CONFIG_SOC_FULL_ICACHE=y
CONFIG_ESP8266_WIFI_RX_BUFFER_NUM=26
CONFIG_ESP8266_WIFI_LEFT_CONTINUOUS_RX_BUFFER_NUM=4
CONFIG_ESP8266_DEFAULT_CPU_FREQ_160=y

CONFIG_LWIP_TCP_SND_BUF_DEFAULT=7300
CONFIG_LWIP_TCP_WND_DEFAULT=7300
CONFIG_LWIP_TCP_MSS=1460
CONFIG_LWIP_TCP_RECVMBOX_SIZE=7

CONFIG_PARTITION_TABLE_CUSTOM_FILENAME="partitions.esp8266.csv"

CONFIG_LOG_SET_LEVEL=y
CONFIG_COMPILER_OPTIMIZATION_LEVEL_RELEASE=y
